{
	"name": "forseti-visualizer-api",
	"version": "2.1.0",
	"description": "Forseti Visualizer API to serve Forseti Based GCP content",
	"author": "Garrett Wong",
	"main": "server.js",
	"scripts": {
		"dev": "nodemon -w server --exec \"babel-node server",
		"build": "babel server -s -D -d dist",
		"serve": "nodemon -w server --exec \"babel-node server\"",
		"start": "npm run serve",
		"test": "babel server.js -d dist && mocha --recursive --exit --require @babel/register",
		"babel-version": "babel --version"
	},
	"eslintConfig": {
		"extends": "eslint:recommended",
		"parserOptions": {
			"ecmaVersion": 7,
			"sourceType": "module"
		},
		"env": {
			"node": true
		},
		"rules": {
			"no-console": 0,
			"no-unused-vars": 1
		}
	},
	"dependencies": {
		"@grpc/proto-loader": "^0.5.4",
		"acorn-globals": "^6.0.0",
		"axios": "^0.19.2",
		"body-parser": "^1.18.3",
		"braces": "^3.0.2",
		"compression": "^1.7.3",
		"cors": "^2.8.4",
		"eslint-utils": "^1.4.3",
		"express": "^4.16.1",
		"express-session": "^1.17.0",
		"grpc": "^1.24.2",
		"lodash": ">=4.17.13",
		"lodash.merge": ">=4.6.2",
		"morgan": "^1.10.0",
		"mysql": "^2.18.1",
		"pug": "^2.0.4",
		"resource-router-middleware": "^0.7.0",
		"session-file-store": "^1.4.0",
		"telnet-client": "^1.4.0",
		"uuid": "^3.4.0"
	},
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/npm/npm.git"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/node": "^7.8.7",
		"@babel/preset-env": "^7.9.5",
		"@babel/register": "^7.9.0",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"eslint": "^5.3.0",
		"js-yaml": ">=3.13.1",
		"minimist": "^1.2.5",
		"mocha": "^6.2.3",
		"nodemon": "^1.19.4",
		"sinon": "^7.5.0",
		"sinon-chai": "^3.5.0"
	}
}
