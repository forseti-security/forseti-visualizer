  },
  "devDependencies": {
    "caniuse-lite": "^1.0.30001042",
    "browserslist": "^4.11.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@vue/cli": "4.1.0-beta.0",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
