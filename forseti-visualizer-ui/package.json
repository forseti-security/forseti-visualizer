    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.2",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
